<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllersViews.table">
   <children>
      <Text layoutX="207.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Income Taxes List">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <TableView fx:id="table" layoutX="62.0" layoutY="73.0" prefHeight="277.0" prefWidth="475.0">
        <columns>

          <TableColumn fx:id="colIncome" prefWidth="120" text="Income" />
          <TableColumn fx:id="colTax" minWidth="0.0" prefWidth="120" text="Tax" />
            <TableColumn fx:id="colNetIncome" prefWidth="130" text="Net Salary" />
            <TableColumn fx:id="colrollNum" prefWidth="190" text="Roll Number" />
        </columns>
      </TableView>
      <Button fx:id="btnBackTable" layoutX="62.0" layoutY="22.0" mnemonicParsing="false" onAction="#changeToMainScreen" prefHeight="27.0" prefWidth="55.0" style="-fx-background-color: #6a0dad;" text="Back" textFill="WHITE" />
      <Button fx:id="btnGetList" layoutX="446.0" layoutY="22.0" mnemonicParsing="false" onAction="#showListInTable" prefHeight="34.0" prefWidth="89.0" style="-fx-background-color: #6a0dad;" text="Refresh" textFill="WHITE">
